{
  "mcpServers": {

    "drawio-mcp": {
      "command": "drawio-mcp"
    },
    "python-analyst": {
      "command": "mcp-language-server",
      "args": [
        "--workspace",
        "./",
        "--lsp",
        "pyright-langserver",
        "--",
        "--stdio"
      ]
    },
    "python-debugger": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "mcp-pdb",
        "mcp-pdb"
      ]
    }
  },
  "general": {
    "previewFeatures": true
  }
}
